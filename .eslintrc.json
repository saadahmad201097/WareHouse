{
  "extends": ["react-app", "airbnb", "prettier", "plugin:react/recommended"],
  "plugins": ["prettier", "react"],
  "rules": {
    "no-unused-vars":"warn",
    "react/jsx-filename-extension":"off",
    "react/jsx-curly-brace-presence":"warn",
    "no-console":"off",
    "no-else-return":"off",
    "no-param-reassign":"off",
    "react/prop-types": "off",
    "func-names":"off"
  }
}
